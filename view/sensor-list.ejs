<section id="page">
    <div id="sensor-list">
        <h3>Sensoren</h3>
        <table>
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Beschreibung</th>
                </tr>
            </thead>
            <tbody>
                <% for (const sensor of sensors) { %>
                <tr>
                    <td><%= sensor.name %></td>
                    <td><%= sensor.description %></td>
                </tr>
                <% } %>
            </tbody>
        </table>
    </div>
</section>
